{
  "name": "creatorx-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:mobile": "tsc && vite build --mode mobile",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cap:sync": "cap sync",
    "cap:sync:ios": "cap sync ios",
    "cap:sync:android": "cap sync android",
    "cap:open:ios": "cap open ios",
    "cap:open:android": "cap open android",
    "mobile:build": "npm run build:mobile && npm run cap:sync",
    "mobile:build:ios": "npm run build:mobile && npm run cap:sync:ios",
    "mobile:build:android": "npm run build:mobile && npm run cap:sync:android",
    "ios:dev": "npm run mobile:build:ios && npm run cap:open:ios",
    "android:dev": "npm run mobile:build:android && npm run cap:open:android"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
